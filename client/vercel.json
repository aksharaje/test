{
    "buildCommand": "npm install && npm run build",
    "outputDirectory": "dist/browser",
    "installCommand": "npm install --include=dev",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://<YOUR_SERVER_URL>/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}